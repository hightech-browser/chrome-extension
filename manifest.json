{
  "name": "Hightech Browser",
  "version": "0.0.1",
  "description": "",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["libs/socket.io.js", "app/main.js"]
    }
  ],
  "permissions": [
    "http://localhost"
  ],
  "browser_action": {
    "default_title": "Google Mail",
    "default_popup": "app/options/index.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "manifest_version": 2
}